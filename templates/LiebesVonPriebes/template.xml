<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Template>
    <Name>Liebes von Priebes</Name>
    <Author>JTL-Software-GmbH</Author>
    <URL>https://www.liebesvonpriebes.de</URL>
    <Version>4.05</Version>
    <ShopVersion>405</ShopVersion>
    <Parent>Evo</Parent>
    <Preview>preview.png</Preview>
    <Description>Template fuer priebes B2B shop</Description>
    <Section Name="Theme" Key="theme">
        <Settings>
            <Setting Description="Theme" Key="theme_default" Type="select" Value="evo">
                <Option Value="evo">Evo - Handgemacht mit <![CDATA[&hearts;]]> von JTL</Option>
                <Option Value="lumen">Lumen - Licht und Schatten</Option>
                <Option Value="priebes">Priebes - Auf Basis von Lumen, abgeaendert fuer Priebes</Option>
            </Setting>
        </Settings>
    </Section>
    <Minify>
        <JS Name="jtl3.js">
            <File Path="js/conf.test.js" DependsOnSettingGroup="CONF_PREISVERLAUF" DependsOnSetting="preisverlauf.preisverlauf_anzeigen" DependsOnSettingValue="Y"/>
            <File Path="js/mytheme.js"/>
            <File Path="js/jtl.basket.js" override="true"/>
        </JS>
        <CSS Name="mytheme.css">
            <File Path="themes/priebes/bootstrap.css"/>
            <File Path="themes/priebes/custom.css"/>
            <File Path="../Evo/themes/base/offcanvas-menu.css"/>
            <File Path="../Evo/themes/base/pnotify.custom.css"/>
            <File Path="../Evo/themes/base/jquery-slider.css"/>
        </CSS>
    </Minify>
    <Boxes>
        <Container Position="right" Available="1"></Container>
    </Boxes>
</Template>
